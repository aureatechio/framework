# Copie este arquivo para `config.env` e preencha os valores.
# IMPORTANTE: `config.env` contém segredos e NÃO deve ser commitado no Git.

# Supabase
SUPABASE_URL=
# Chave service_role (somente para scripts de deploy no servidor/PC; nunca no browser)
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_BUCKET_NAME=cdn-assets

# Layout de upload no Storage:
# - per_element (padrão): <nome>/<versao>/...  (ex.: dashboard/v2/form.js)
# - legacy: <versao>/widgets/<widget_slug>/... (layout antigo)
STORAGE_LAYOUT=per_element
# Prefixo opcional dentro do bucket (ex.: "cdn", "assets"). Deixe vazio para raiz.
STORAGE_PREFIX=

# Bubble (Data API)
BUBBLE_OBJ_URL=
BUBBLE_TOKEN=

# Bubble field keys (ajuste conforme nomes internos expostos na Data API)
BUBBLE_FIELD_NOME=elemento_text
BUBBLE_FIELD_CSS=css_text
BUBBLE_FIELD_JS=js_text
BUBBLE_FIELD_ATIVO=ativo_boolean
BUBBLE_FIELD_HTML=html_text
BUBBLE_FIELD_VERSION=version_text

# Campos opcionais (se existirem no Bubble / estiverem expostos)
BUBBLE_FIELD_CODE_VERSION=
BUBBLE_FIELD_MANIFEST_URL=

